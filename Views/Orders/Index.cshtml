@model IEnumerable<MatureFashionsAppV2.Models.DB.Orders>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrderDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FranchiseNoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FranchiseNoNavigation.FranchiseNo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.OrderNo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.OrderNo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.OrderNo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
